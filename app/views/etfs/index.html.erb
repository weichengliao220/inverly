<h1 class="text-center my-4">Welcome to your ETF Index</h1>
<div class="container">
  <div class="row">
    <% @etfs.each do |etf| %>
      <div class="col-md-4 mb-4">
        <div class="etf-card card">
          <div class="card-body">
            <div class="footer-title-container">
              <h5 class="card-title"><%= link_to etf.name, etf_path(etf), style: "text-decoration: none;" %></h5>
              <span class="line"></span>
            </div>
            <div class="icon">
              <strong>Prev Close Price: $<%= etf.latest_monthly_time_price %></strong>
              <small style="background: linear-gradient(to right, #808080, #d3d3d3); -webkit-background-clip: text; color: transparent;">00</small>
            </div>
            <h6 class="card-subtitle mb-2 text-muted"><%= etf.ticker_symbol %></h6>
            <p class="card-text"><%= etf.description %></p>
          </div>
          <div class="card-footer d-flex justify-content-between align-items-center">
            <%= link_to 'View Details', etf_path(etf), class: 'btn text-white' %>
            <% if current_user.favorite_etfs.include?(etf) %>
              <%= button_to etf_favorite_path(etf), method: :delete, class: 'btn' do %>
                <i class="fa-regular fa-heart text-danger"></i>
              <% end %>
            <% else %>
              <%= button_to etf_favorite_path(etf), method: :post, class: 'btn' do %>
                <i class="far fa-heart text-light"></i>
              <% end %>
            <% end %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
